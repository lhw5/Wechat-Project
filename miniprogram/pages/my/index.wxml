<view class="view_contain">
  <!-- 第一部分 -->
  <view class="view_1">
    <view class="view_image_text">
      <view class="image_radius">
        <open-data type="userAvatarUrl"></open-data>
      </view>
      <open-data type="userNickName"></open-data>
    </view>
  </view>
  <!-- 第二部分 -->
  <view class="view_2">
    <view class="view_tupianwenzi">
      <image class="image_tupian" src="../my/image/sc.png"></image>
      <text class="text_saoyisao">收藏</text>
    </view>
    <navigator url="/pages/my/setup/index" hover-class='none'>
      <view class="view_tupianwenzi">
        <image class="image_tupian" src="../my/image/sz.png"></image>
        <text class="text_saoyisao">设置</text>
      </view>
    </navigator>

  </view>
  <!-- 第三部分 -->
  <view class="view_3">
    <navigator url="/pages/my/about/index">
      <view class="list-item">
        <image class="item-image" src="../my/image/gywm.png"></image>
        <text class="item-text">关于我们</text>
        <image class="image-jiantou" src="../my/image/jt.png"></image>
      </view>
    </navigator>
    <view class="line"></view>
    <navigator url="/pages/my/info/index">
      <view class="list-item">
        <image class="item-image" src="../my/image/grxx.png"></image>
        <text class="item-text">个人信息</text>
        <image class="image-jiantou" src="../my/image/jt.png"></image>
      </view>
    </navigator>
    <view class="line"></view>
    <navigator url="/pages/my/suggest/index">
      <view class="list-item">
        <image class="item-image" src="../my/image/jy.png"></image>
        <text class="item-text">建议和反馈</text>
        <image class="image-jiantou" src="../my/image/jt.png"></image>
      </view>
    </navigator>
    <view class="line"></view>
  </view>
  <!-- 第四部分 -->
  <view bindtap="loginoutClick" class="view_4">退出登录</view>

</view>